<ul class="collection">
    <li class="collection-header"><div class="row"><div class="col s2"></div><div class="col s8">Joueurs</div>{#<div class="col s2">M.</div>#}<div class="col s2">B.</div></div></li>
    {% for eventContainer in events %}
    <li class="collection-item "><div class="row"><div class="col s2">{{ loop.index }}</div><div class="col s8">{{ eventContainer[0].player }}{% if is_granted('ROLE_ADMIN') %}
                    <a href="{{ path('edit-player',{id:eventContainer[0].player.id }) }}"><i class="material-icons">edit</i></a>
                {% endif %}</div>{#<div class="col s2">5</div>#}<div class="col s2">{{ eventContainer.goals }}</div></div></li>
    {% endfor %}
</ul>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ url('homepage') }}</loc>
        <changefreq>weekly</changefreq>
        <priority>1</priority>
    </url>
    {% for match in matchs %}
    <url>
        <loc>{{ url('view_match',{id:match.id}) }}</loc>
        <priority>0.8</priority>
    </url>
    {% endfor %}
</urlset>